{{#partial "content"}}
    <form action="/ideas/{{idea.slug}}/vote" method="post">
        <h1>{{idea.title}}</h1> ( {{idea.voteCount}} votes ) <button>Vote</button>
    </form>
    <h2>Voters</h2>
    <ul>
        {{#each idea.voters}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
{{/partial}}

{{#partial "title"}}
    Course Idea - {{idea.title}}
{{/partial}}

{{> base.hbs}}